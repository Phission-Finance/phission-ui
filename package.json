{
  "name": "phission-ui",
  "private": false,
  "version": "0.1.0",
  "homepage": "https://phission.github.io/phission-ui",
  "scripts": {
    "predeploy": "npm run build && npm run export ",
    "deploy": "gh-pages --no-history -d build -t true",
    "dev": "NODE_OPTIONS='--inspect'  next dev",
    "build": "next build ",
    "export": "next export -o build/ && touch ./build/.nojekyll && cp ./CNAME ./build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^0.4.6",
    "@sentry/nextjs": "^7.11.1",
    "bootstrap": "^5.2.0",
    "ethers": "^5.0.0",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.1.0",
    "wagmi": "^0.6.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "gh-pages": "^4.0.0",
    "typescript": "^4.7.2"
  }
}
